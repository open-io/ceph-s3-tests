[default]
region = ${CDS_ENV_S3_REGION}
s3 =
    addressing_style = virtual
    max_concurrent_requests = 10
    max_queue_size = 100
    multipart_threshold = 15MB
    multipart_chunksize = 5MB
    signature_version = s3v4

[profile ${CDS_ENV_U0_USERNAME}]
region = ${CDS_ENV_S3_REGION}
s3 =
    addressing_style = virtual
    max_concurrent_requests = 10
    max_queue_size = 100
    multipart_threshold = 15MB
    multipart_chunksize = 5MB
    signature_version = s3v4

