[DEFAULT]
# host set for rgw in vstart.sh
host = %S3_SERVER%

# port set for rgw in vstart.sh
port = %S3_PORT%

## say "False" to disable TLS
is_secure = True

## say "False" to disable SSL Verify
ssl_verify = True

[fixtures]
## all the buckets created will start with this prefix;
## {random} will be filled with random characters to pad
## the prefix to 30 characters long, and avoid collisions
bucket prefix = s3-tests-{random}-

[s3 main]
# main display_name set in vstart.sh
display_name = %S3_PROJECT_NAME%:s3-tests-admin

# main user_idname set in vstart.sh
user_id = %S3_PROJECT_NAME%:s3-tests-admin

# main email set in vstart.sh
email = tester@ceph.com

# zonegroup api_name for bucket location
api_name = default

## main AWS access key
access_key = %U0_ACCESS%

## main AWS secret key
secret_key = %U0_SECRET%

## replace with key id obtained when secret is created, or delete if KMS not tested
#kms_keyid = 01234567-89ab-cdef-0123-456789abcdef

## Storage classes
#storage_classes = "LUKEWARM, FROZEN"

## Lifecycle debug interval (default: 10)
#lc_debug_interval = 20

[s3 alt]
# alt display_name set in vstart.sh
display_name = %S3_PROJECT_NAME%:s3-tests-user1
## alt email set in vstart.sh
email = john.doe@example.com

# alt user_id set in vstart.sh
user_id = %S3_PROJECT_NAME%:s3-tests-user1

# alt AWS access key set in vstart.sh
access_key = %U1_ACCESS%

# alt AWS secret key set in vstart.sh
secret_key = %U1_SECRET%

[s3 tenant]
# tenant display_name set in vstart.sh
display_name = testx$tenanteduser

# tenant user_id set in vstart.sh
user_id = %S3_PROJECT_NAME%:s3-tests-admin

# tenant AWS secret key set in vstart.sh
access_key = %U0_ACCESS%

# tenant AWS secret key set in vstart.sh
secret_key = %U0_SECRET%

# tenant email set in vstart.sh
email = tenanteduser@example.com
